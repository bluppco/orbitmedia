---
// IMPORT ASTRO LAYOUTS
import Container from "../../layouts/Container.astro"

// IMPORT ASTRO COMPONENTS
import OptimizeWebsiteItem from "./OptimizeWebsiteItem.astro"

const { data } = Astro.props
---
<Container>
    <OptimizeWebsiteItem
        title={ data.heading }
        sub_title={ data.title }
        description={ data.description }
        src={ data.cover }
        alt={ data.heading }
    />
</Container>

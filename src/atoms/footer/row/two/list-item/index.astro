---

// IMPORTS ASTRO ATOMS
import Link from "@/atoms/links/index.astro"

// GET ASTRO PROPS
const {

    aria_label,
    href

} = Astro.props

---
<li class="text-center md:text-left text-sm list-none md:list-disc text-om_orange hover:text-white font-bold font-proxima_nova transition-all duration-200 cursor-pointer">
    <Link href={ href } aria_label={ aria_label }>
        <slot/>
    </Link>
</li>
